{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "startdev": "nodemon r ./bin/www",
    "test": "mocha",
    "start_socket": "nodemon app.js",
    "build": "tsc",
    "clean": "rimraf build/",
    "copy-files": "copyfiles -u 1 src/**/**/**/**.json src/**/*.css build/src/",
    "build-ts": "npm run clean && tsc && npm run copy-files && node build/src/app.js",
    "socket-ts": "tsc && node build/src/app.js"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "ansi-escapes": "^6.0.0",
    "chai": "^4.3.7",
    "chai-spies": "^1.0.0",
    "circular-json": "^0.5.9",
    "clear": "^0.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "flatted": "^3.2.7",
    "inquirer": "^9.1.4",
    "json-stringify-safe": "^5.0.1",
    "mocha": "^10.2.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.20",
    "prompt": "^1.3.0",
    "socket.io": "^4.6.1",
    "ts-node-dev": "^2.0.0",
    "tsc-hooks": "^1.1.2",
    "typescript": "^5.1.6",
    "util": "^0.12.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.4",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.1"
  }
}
